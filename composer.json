{
  "name": "tejadong/graphql-mutation-validator-bundle",
  "description": "Provide a base class to validate user data sent in a GraphQL mutation and return validations errors. (Symfony 3)",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["GraphQL", "Validation"],
  "authors": [
    {
      "name": "Tejadong",
      "homepage": "https://www.Tejadong.com"
    },
    {
      "name": "Sylvain Fabre (author owner)"
    },
    {
      "name": "Jesus Tejada (author modifier)"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tejadong\\GraphQLMutationValidatorBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require": {
    "php": "^7.3",
    "overblog/graphql-bundle": "^0.11",
    "symfony/validator": "~3.0",
    "doctrine/annotations": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "symfony/twig-bundle": "~3.0",
    "symfony/security-bundle": "~3.0",
    "symfony/yaml": "~3.0",
    "symfony/templating": "~3.0",
    "symfony/console": "~3.0"
  }
}
